    "@solar-republic/bc-ur-registry": "^0.5.4-patch.2",
    "@solar-republic/bc-ur-registry-cosmos": "^0.2.2-patch.2",
    "@solar-republic/cosmos-grpc": "0.6.3",
    "@solar-republic/grpc-web": "^0.15.0-fix.3",
    "@solar-republic/svelte-toggle": "^3.1.0",
    "@solar-republic/wasm-secp256k1": "^0.0.3-beta.2",
    "@zerodevx/svelte-json-view": "^1.0.5",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.1.1",
    "deepmerge": "^4.2.2",
    "fuse.js": "^6.6.2",
