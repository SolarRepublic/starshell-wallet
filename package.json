{
  "name": "starshell-wallet-extension",
  "displayName": "StarShell Wallet",
  "private": true,
  "type": "module",
  "version": "1.0.34",
  "description": "A privacy-preserving, free and open-source Web3 wallet built for the Secret Network and broader Cosmos ecosystem",
  "license": "MIT",
  "author": {
    "name": "Blake Regalia",
    "email": "blake@starshell.net",
    "url": "https://blake-regalia.net/"
  },
  "contributors": [],
  "eslintIgnore": [
    "dist",
    "node_modules",
    "submodules",
    ".eslintrc.cjs",
    "comments.ts",
    "vite.config.ts"
  ],
  "scripts": {
    "lint": "eslint --ext .ts,.svelte .",
    "build": "scripty",
    "build:chrome": "SCRIPTY_PARALLEL=true scripty",
    "build:android": "yarn build:chrome android-",
    "build:firefox": "SCRIPTY_PARALLEL=true scripty",
    "build:firefox-beta": "SCRIPTY_PARALLEL=true scripty",
    "build:safari": "SCRIPTY_PARALLEL=true scripty",
    "build:safari-beta": "SCRIPTY_PARALLEL=true scripty",
    "watch:android": "ENGINE=android-chrome vite build --mode development --watch --",
    "watch:chrome": "ENGINE=chrome vite build --mode development --watch --",
    "watch:firefox": "ENGINE=firefox vite build --mode development --watch --",
    "watch:safari": "ENGINE=safari vite build --mode development --watch --",
    "watch:safari-beta": "ENGINE=safari vite build --mode beta --watch --",
    "dev:chrome": "ENGINE=chrome vite build --mode development --",
    "dev:firefox": "ENGINE=firefox vite --",
    "dev:safari": "yarn watch:safari --",
    "serve:chrome": "ENGINE=chrome web-ext run -t chromium --start-url \"https://google.com\" --source-dir ./dist/chrome",
    "serve:firefox": "scripty",
    "serve:android-firefox": "scripty",
    "deploy:android-kiwi-webkit": "yarn build:safari && adb push dist/safari.zip /sdcard/Download",
    "deploy:android-kiwi-chromium": "yarn build:chrome && adb -e push dist/chrome.zip /sdcard/Download",
    "host:page": "sirv local --dev --port=8188",
    "host:chrome": "sirv dist/chrome --dev --port=8128",
    "clear": "clear (){ rm -rf dist/$1/** ; }; clear",
    "test": "vitest",
    "check": "svelte-check"
  },
  "devDependencies": {
    "@cosmjs/amino": "^0.30.1",
    "@cosmjs/proto-signing": "^0.30.1",
    "@keplr-wallet/provider": "^0.11.63",
    "@keplr-wallet/router": "^0.11.63",
    "@keplr-wallet/types": "^0.11.63",
    "@keplr-wallet/wc-client": "^0.11.63",
    "@ledgerhq/errors": "^6.12.6",
    "@ledgerhq/hw-transport-webusb": "^6.27.15",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.1",
    "@rollup/pluginutils": "^5.0.2",
    "@samrum/vite-plugin-web-extension": "^3.1.1",
    "@sveltejs/vite-plugin-svelte": "2.4.1",
    "@types/chrome": "^0.0.237",
    "@types/crypto-js": "^4.1.1",
    "@types/estree": "^1.0.1",
    "@types/libsodium-wrappers": "^0.7.10",
    "@types/ua-parser-js": "^0.7.36",
    "@types/w3c-web-usb": "^1.0.6",
    "@types/web": "^0.0.99",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "acorn-walk": "^8.2.0",
    "archiver": "^5.3.1",
    "crypto-js": "^4.1.1",
    "esbuild": "^0.17.19",
    "eslint": "^8.41.0",
    "eslint-plugin-i": "^2.27.5",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "jsdom": "^21.1.1",
    "less": "^4.1.3",
    "magic-string": "^0.30.0",
    "postcss": "^8.4.24",
    "rollup": "3.23.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-graph": "^2.0.0",
    "rollup-plugin-node-externals": "^6.0.1",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "scripty": "^2.1.1",
    "secretjs": "^1.9.0",
    "ses": "0.15.17",
    "sirv": "^2.0.3",
    "svelte": "^3.59.1",
    "svelte-check": "^3.4.3",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.2",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^5.0.4",
